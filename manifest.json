{
  "manifest_version": 3,
  "name": "Wyze Autoplay",
  "version": "1.1",
  "description": "Autoplay Wyze videos",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://view.wyze.com/*"
      ]
    }
  ]
}
